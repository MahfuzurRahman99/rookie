<h1>Hello Header 2</h1>
<div class="s-main-wrapper">
  <div class="s-logo-wrapper">
    <a class="s-logo-wrapper-link" href="#"
      ><img class="s-logo-wrapper-img" src="{{ 'final_logo.jpg' | asset_url }}" alt="Scratch Logo"
    ></a>
  </div>
  <div class="s-menu-wrapper">
    <ul class="s-menu-wrapper-ul">
      <li><a href="">Home</a></li>
      <li><a href="">product</a></li>
      <li><a href="">store</a></li>
      <li><a href="">category</a></li>
      <li><a href="">contact</a></li>
    </ul>
  </div>
</div>

<style>
    .s-main-wrapper {
      max-width: 1200px;
      margin: 0 auto;
      display: flex;
      justify-content: space-between;
      align-items: center;
    }
    ,
    .s-logo-wrapper-link{
      display: block;
      width: 100px;
      height: 100px;
  },
    .s-logo-wrapper-img{
      max-width: 100%;
  },
    .s-menu-wrapper-ul {
      display: flex;
      justify-content: space-between;
      align-items: center;
    }
    ,
    {% comment %} .s-menu-wrapper ul li {
      display: block;
    } {% endcomment %}
</style>

{% schema %}
{
  "name": "Header-scratch",
  "class": "header-scratch",
  "tag": "header",
  "settings": []
}
{% endschema %}
